#include <stdio.h>
/**
* main - Entry Point
* Description: print 1 - 100, multiples of 3 print fizz, multiples of 5 is buzz
* Return: Always 0 (Success)
*/
int main(void)
{
	int num;

	for (num = 1; num < = 100; num++)
	{
		if(num % 3 == 0)
		{
			printf("fizz ");
		}
		if(num % 5 == 0)
		{
			printf("buzz ");
		}
	}
	return (0);
}
